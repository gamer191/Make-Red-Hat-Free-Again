+        let fade1 = new St.Widget({ style_class: 'scrollview-fade', reactive: false });
+        let fade2 = new St.Widget({ style_class: 'scrollview-fade', 
+                                    reactive: false,  
+                                    pivot_point: Utils.getPoint({ x: .5, y: .5 }), 
