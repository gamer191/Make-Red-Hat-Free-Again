+    let padding = Me.settings.get_int('appicon-padding');
+    let availSize = panelSize - Taskbar.MIN_ICON_SIZE - panelSize % 2;
+
+    if (padding * 2 > availSize) {
+        padding = availSize * .5;
+    }
+
+    return padding;
+}
