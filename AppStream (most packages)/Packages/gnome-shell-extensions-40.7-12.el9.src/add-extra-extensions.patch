+        }
+
+        if (this._destroyId > 0) {
+            this._mutterWindow.disconnect(this._destroyId);
+            this._destroyId = 0;
+        }
+
+        if (this._windowTitleId > 0) {
+            this._window.disconnect(this._windowTitleId);
+            this._windowTitleId = 0;
+        }
+    }
