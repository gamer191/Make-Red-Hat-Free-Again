+            let tweenOpts = { 
+                time: Taskbar.DASH_ANIMATION_TIME,
+                transition: 'easeInOutCubic',
+                onComplete: Lang.bind(this, function() { 
+                    if(newOtherOpacity > 0)
+                        otherDots.opacity = newOtherOpacity;
+                    dots._tweeningToSize = null;
+                })
+            };
+
+            if(newOtherOpacity == 0)
+                otherDots.opacity = newOtherOpacity;
