+    }
+
+    destroy() {
+        this._signalsHandler.destroy();
+        this._transparency.destroy();
+    }
+
+    _onOverviewShowing() {
+        this._actor.add_style_pseudo_class('overview');
+    }
+
+    _onOverviewHiding() {
+        this._actor.remove_style_pseudo_class('overview');
+    }
